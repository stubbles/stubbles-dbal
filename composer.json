{
    "name": "stubbles/dbal",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Database abstraction layer.",
    "require": {
        "php": ">=5.6.0",
        "ext-pdo": "*",
        "stubbles/core": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1",
        "mikey179/vfsStream": "~1.6",
        "bovigo/callmap": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\db\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
