{
    "name": "stubbles/dbal",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Database abstraction layer.",
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "stubbles/ioc": "^9.0",
        "stubbles/sequence": "^9.0",
        "stubbles/values": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "mikey179/vfsStream": "^1.6.8",
        "bovigo/callmap": "^5.1",
        "bovigo/assert": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\db\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\db\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
