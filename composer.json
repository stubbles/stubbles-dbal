{
    "name": "stubbles/dbal",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Database abstraction layer.",
    "require": {
        "php": "^7.0",
        "ext-pdo": "*",
        "stubbles/ioc": "^8.0",
        "stubbles/sequence": "^8.0",
        "stubbles/values": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mikey179/vfsStream": "^1.6",
        "bovigo/callmap": "^3.1",
        "bovigo/assert": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\db\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
