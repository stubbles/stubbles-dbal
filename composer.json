{
    "name": "stubbles/dbal",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Database abstraction layer.",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "stubbles/sequence": "^11.0",
        "stubbles/values": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "mikey179/vfsstream": "^1.6.12",
        "bovigo/callmap": "^9.0",
        "bovigo/assert": "^8.1",
        "stubbles/ioc": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\db\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\db\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "12.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
