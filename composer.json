{
    "name": "stubbles/dbal",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Database abstraction layer.",
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "stubbles/sequence": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mikey179/vfsstream": "^1.6.8",
        "bovigo/callmap": "^6.1",
        "bovigo/assert": "^6.0",
        "stubbles/values": "^9.1",
        "stubbles/ioc": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\db\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\db\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "php-coveralls": "php-coveralls"
    }
}
